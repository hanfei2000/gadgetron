    "version": "0.2.0",
    "configurations": [
        {
            "name": "gadgetron server",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/local/bin/gadgetron",
            "args": ["-p", "9006", "-W", "/home/hanfei/recon/debug"],
            "stopAtEntry": false,
            "cwd": "/home/hanfei/recon/debug/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}